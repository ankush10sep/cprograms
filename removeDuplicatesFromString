#include "stdio.h"

#define MAX_CHARS 256
char * removeDups(char *str)
{
   char hash[MAX_CHARS] = {0};
   int i = 0;
   int res_index = 0;

   while (*(str+i))
   {
      if (hash[*(str+i)] == 0)
      {
         hash[*(str+i)] = 1;
         str[res_index] = str[i];
         res_index++;
      }
      i++;
   }

   str[res_index] = '\0';

   return str;
}

int main()
{
   char str[] = "geeksforgeeks";
   printf("%s\n", removeDups(str));
   return 0;
}

Output: geksfor
